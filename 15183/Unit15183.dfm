        'y'#39')    '
      '           &date_filter'
      '),'
      ''
      'PPWUN AS ('
      '         Select '
      '                CASE '
      '                    WHEN (end_work - start_work) = 0'
      '                    THEN 0.5 / 60'
      '                    ELSE  (end_work - start_work)* 24 * 60'
      '                END Time_Work_MINUTES,'
      '                to_char(start_work, '#39'HH24'#39') as hour_stark_work,'
      '                to_char(end_work, '#39'HH24'#39') as hour_end_work,'
      '                tab.*'
      '         FROM tab       '
      '),'
      ''
      'initial_data as ('
      '    Select  PPWUN.operating_day,'
      '            PPW.tab_num,'
      '            PPDOL.title_short,'
