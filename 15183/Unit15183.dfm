        '      sum( DECODE(hour_stark_work, 10, diff_time_deviation)) as ' +
        'ten,'
      
        '      sum( DECODE(hour_stark_work, 11, diff_time_deviation)) as ' +
        'eleven,'
      
        '      sum( DECODE(hour_stark_work, 12, diff_time_deviation)) as ' +
        'twelve,'
      
        '      sum( DECODE(hour_stark_work, 13, diff_time_deviation)) as ' +
        'thirteen,'
      
        '      sum( DECODE(hour_stark_work, 14, diff_time_deviation)) as ' +
        'fourteen,'
      
        '      sum( DECODE(hour_stark_work, 15, diff_time_deviation)) as ' +
        'fifteen,'
      
        '      sum( DECODE(hour_stark_work, 16, diff_time_deviation)) as ' +
        'sixteen,'
      
        '      sum( DECODE(hour_stark_work, 17, diff_time_deviation)) as ' +
