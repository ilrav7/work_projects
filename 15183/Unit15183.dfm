  object orqryTimeDev: TOraQuery
    Session = DM_PlanPers.OraSession1
    SQL.Strings = (
      'SELECT '
      '       PPWL.id_work,'
      '       PPWL.work_name, '
      '       PPWL.measure_unit,'
      '       PPD.id_dept,'
      '       pptd.month_deviation,'
      '       pptd.year_deviation,'
      '       pptd.time_deviation,'
      '       DECODE(pptd.month_deviation, '
      '              1, '#39#1103#1085#1074#1072#1088#1100#39','
      '              2, '#39#1092#1077#1074#1088#1072#1083#1100#39','
      '              3, '#39#1084#1072#1088#1090#39','
      '              4, '#39#1072#1087#1088#1077#1083#1100#39','
      '              5, '#39#1084#1072#1081#39','
      '              6, '#39#1080#1102#1085#1100#39','
      '              7, '#39#1080#1102#1083#1100#39','
      '              8, '#39#1072#1074#1075#1091#1089#1090#39','
      '              9, '#39#1089#1077#1085#1090#1103#1073#1088#1100#39','
      '              10, '#39#1086#1082#1090#1103#1073#1088#1100#39','
      '              11, '#39#1085#1086#1103#1073#1088#1100#39','
      '              12, '#39#1076#1077#1082#1072#1073#1088#1100#39') as MONTH_W,'
      '        pptd.id_deviation,'
      '        PPD.Dept_name_short'
      ' '
      'FROM plan_pers_works_list PPWL'
      '    INNER JOIN plan_pers_dept PPD '
      '        ON PPWL.id_dept = PPD.id_dept'
      '    LEFT JOIN plan_pers_time_deviation pptd'
      '        ON pptd.id_work = PPWL.id_work '
      '        AND PPWL.id_dept = pptd.id_dept'
      '        &month_filter'
      '        &year_filter'
      '    INNER JOIN user_access_dept uad'
      '        ON uad.id_dept = PPWL.id_dept'
      '          --  AND uad.id_user = 46023 '
      '        AND uad.id_user = :nom_user'
      'WHERE  PPWL.work_unit = '#39#1057#1044'/RV'#39
      '       AND PPWL.active = 1'
      '       &dept_filter'
      'Order By pptd.month_deviation,'
      '         pptd.year_deviation')
    FetchAll = True
    Left = 1432
    Top = 480
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'month_id'
      end
      item
        DataType = ftUnknown
        Name = 'year_id'
      end
      item
        DataType = ftUnknown
        Name = 'nom_user'
      end
      item
        DataType = ftUnknown
        Name = 'dept_id'
      end>
    MacroData = <
      item
        Name = 'month_filter'
        Value = 'AND pptd.month_deviation = :month_id'
      end
      item
        Name = 'year_filter'
        Value = 'AND pptd.year_deviation = :year_id'
      end
      item
        Name = 'dept_filter'
        Value = 'AND ppd.id_dept = :dept_id'
      end>
  end
