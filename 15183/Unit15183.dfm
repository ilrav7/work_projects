        '      sum( DECODE(hour_stark_work, 1, diff_time_deviation)) as o' +
        'ne,'
      
        '      sum( DECODE(hour_stark_work, 2, diff_time_deviation)) as t' +
        'wo,'
      
        '      sum( DECODE(hour_stark_work, 3, diff_time_deviation)) as t' +
        'hree,'
      
        '      sum( DECODE(hour_stark_work, 4, diff_time_deviation)) as f' +
        'our,'
      
        '      sum( DECODE(hour_stark_work, 5, diff_time_deviation)) as f' +
        'ive,'
      
        '      sum( DECODE(hour_stark_work, 6, diff_time_deviation)) as s' +
        'ix,'
      
        '      sum( DECODE(hour_stark_work, 7, diff_time_deviation)) as s' +
