      '        --  AND ppd.id_dept = 3'
      '          '
      '          --AND lower(PPW.full_name) like '#39#1096#1086#1084#1080#1085'%'#39
      '          &dept_FILTER'
      '          &deptUSER_filter  '
      '          &fio_filter'
      ')'
      ''
      'select operating_day,'
      '       tab_num,'
      '       title_short,'
      '       FULL_NAME,'
      '       DEPT_USER, -- '#1087#1086#1076#1088#1072#1079#1076#1077#1083#1077#1085#1080#1077' '#1089#1086#1090#1088#1091#1076#1085#1080#1082#1072
      '       dept_name_short,'
      '      -- hour_end_work,'
      '      --  diff_time_deviation,'
      '      --  hour_stark_work,'
